# Solar System Explorer - Environment Variables

# Development
NODE_ENV=development
VITE_APP_TITLE="Solar System Explorer"
VITE_APP_VERSION="2.0.0"

# API Configuration (for future NASA integration)
# VITE_NASA_API_KEY=your_nasa_api_key_here
# VITE_EXOPLANET_API_URL=https://exoplanetarchive.ipac.caltech.edu/cgi-bin/nstedAPI/nph-nstedAPI

# Performance Settings
VITE_MAX_ASTEROIDS=1000
VITE_ENABLE_SHADOWS=true
VITE_ENABLE_POSTPROCESSING=true

# Debug Settings
VITE_DEBUG_MODE=false
VITE_SHOW_FPS=false
VITE_ENABLE_STATS=false
